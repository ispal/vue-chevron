{
  "name": "vue-chevron",
  "version": "0.1.0",
  "description": "Animated chevron toggle component",
  "repository": {
    "url": "https://github.com/ispal/vue-chevron",
    "type": "git"
  },
  "main": "dist/vue-chevron.common.js",
  "module": "dist/vue-chevron.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "test-vue-app",
    "build": "bili --compress umd",
    "example": "poi",
    "build:example": "poi build",
    "commit": "git-cz",
    "publish-docs": "gh-pages -d docs/ -b gh-pages",
    "release": "release-it"
  },
  "author": {
    "name": "Ispal",
    "email": "irpa.oss@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "docs",
    "homepage": "./"
  },
  "bili": {
    "moduleName": "VueChevron",
    "banner": true,
    "format": [
      "cjs",
      "umd",
      "es"
    ],
    "plugins": [
      "vue"
    ],
    "vue": {
      "css": "dist/vue-chevron.css"
    }
  },
  "devDependencies": {
    "bili": "0.19.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eases": "1.0.8",
    "flexboxgrid-sass": "8.0.5",
    "gh-pages": "1.1.0",
    "node-sass": "4.7.2",
    "normalize.css": "7.0.0",
    "poi": "9.5.4",
    "prism-themes": "1.0.0",
    "prismjs": "1.8.4",
    "release-it": "5.0.0",
    "rollup-plugin-vue": "2.5.2",
    "sass-loader": "6.0.6",
    "test-vue-app": "1.0.0",
    "vue-prism-component": "1.0.1",
    "vue-switches": "2.0.1",
    "vue-test-utils": "1.0.0-beta.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
